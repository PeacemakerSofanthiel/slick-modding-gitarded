<!-- COURIERS 1 -->
 <BattleCardAbility ID="DiceCardSelfAbility_SlickMod_Courier_DiscardUseLight1">
	<Desc>Restore 1 Light upon discarding this page\n[On Use] Restore 1 Light</Desc>
 </BattleCardAbility>

 <BattleCardAbility ID="DiceCardSelfAbility_SlickMod_Courier_LuckySlipPage">
	<Desc>[On Use] Gain 1 Haste next Scene; Gain 1 Cycle next Scene; If Speed is higher than the target's, all dice on this page gain +2 minimum roll value</Desc>
 </BattleCardAbility>

 <BattleCardAbility ID="DiceCardSelfAbility_SlickMod_Courier_Cycle3Draw1">
	<Desc>[On Use] Gain 3 Cycle next Scene; Draw 1 page</Desc>
 </BattleCardAbility>

 <BattleCardAbility ID="DiceCardSelfAbility_SlickMod_FuckOff">
	<Desc>Draw 1 page upon discarding this page\n[On Use] If user has Cycle, all dice on this page gain +1 Power</Desc>
 </BattleCardAbility>

<!-- SNOW COYOTES -->
 <BattleCardAbility ID="DiceCardSelfAbility_SlickMod_Trudge">
	<Desc>[On Use] Restore 1 Light; If Speed is 2 or lower, boost the minimum value of all dice on this page by 1</Desc>
 </BattleCardAbility>
  
